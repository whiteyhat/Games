pico-8 cartridge // http://www.pico-8.com
version 16
__lua__
--crypto craft
--by satoshis games

version=""
permadeath = true

lb4 = false
lb5 = false

time = 0

function cmenu(s,te1,te2)
	return {spr=s,text=te1,text2=te2}
end

mainmenu=cmenu(0,"by satoshis games","2018")
intromenu=cmenu(8,"mt gox left you","on a deserted island")
intro2menu=cmenu(128,"full of fiat","and monsters")
deathmenu=cmenu(128,"you have been","defeated ...")
winmenu=cmenu(136,"you escaped","but the monster still lives")
win2menu=cmenu(136,"you escaped","in a boat full of treasure")

gamestate = 0

function _init()

	cartdata("nusan_pcraft"..version)
	gamestate = dget(0)

	choices={}
	if(dget(4)==1) add(choices,"load game")
	add(choices,"new game")

	--memcpy(0x2000,0x1000,0x1000)
	--cstore()

	curmenu = mainmenu
	if gamestate==666 then
		music(4)
		memcpy(0x1000,0x2000,0x1000)
		curmenu = deathmenu
	elseif gamestate==999 then
		music(4)
		memcpy(0x1000,0x2000,0x1000)
		curmenu = winmenu
	elseif gamestate==333 then
		music(4)
		curmenu = win2menu
	else
		music(4,10000)
	end
end

selectindex=0

function _update()

	if curmenu then
		if curmenu==mainmenu then
			if(btnp(2)) selectindex+=1
			if(btnp(3)) selectindex-=1
			selectindex = selectindex%(#choices)
		end
		local press4 = btnp(5) and not lb4
		local press5 = btnp(4) and not lb5
		if press4 or press5 then
			if curmenu==mainmenu then
				if selectindex==1 or #choices==1 then
					curmenu=intromenu
				else
					dset(0,1) -- load
					load("pcraft2_main"..version)
				end
			elseif curmenu==intromenu then
				curmenu=intro2menu
			elseif curmenu==intro2menu then
				dset(0,0) -- new
				dset(19,permadeath and 1 or 0)
				load("pcraft2_generation"..version)
			else
				dset(0,0)
				reload()
				curmenu=mainmenu
			end
		end
		lb4 = btn(5)
		lb5 = btn(4)
	end
end

function printc(t,x,y,c)
	print(t,x-#t*2,y,c)
end

function printcb(t,x,y,c,c2)
	x-=#t*2
	print(t,x-1,y,c2)
	print(t,x+1,y,c2)
	print(t,x,y-1,c2)
	print(t,x,y+1,c2)
	print(t,x,y,c)
end

function _draw()

	camera()
	pal()
	palt(0,false)
	rectfill(0,0,128,46,12)
	rectfill(0,46,128,128,1)
	if curmenu==intromenu then
		for i=3,8 do
			--pal(i, i==(flr(time*3)%16+3) and 7 or 13)
			pal(i, rnd()>0.97 and 7 or 13)
		end
	end
	spr(curmenu.spr,32,14,8,8)
	pal()
	printc(curmenu.text, 64,80,6)
	printc(curmenu.text2, 64,87,6)

	--print(gamestate,0,0,8)

	if curmenu==mainmenu then
		printcb("deluxe edition",64,2,12,1)
		local offset = #choices==1 and 8 or 4
		for i=1,#choices do
			printc(choices[i], 64,88+offset+i*8,(i==selectindex+1) and 6+time%2 or 6)
		end
		printc("press z to start", 64,96+24,13)
	else
		printc("press z", 64,102,6+time%2)
		if curmenu==intromenu then
			printc("enter : access save menu", 64,96+24,13)
		elseif curmenu==intro2menu then
			printc("i : access inventory", 64,96+24,13)
		end
	end
	time += 0.1

	--if(true) then
	--	print("cpu "..flr(stat(1)*100),96,0,8)
	--	print("ram "..flr(stat(0)),96,8,8)
	--end

end

